        },
        {
          "label": "<b>Do you ensure any personal or medical information provided  by a food handler is kept confidential?</b>",
          "optionsLabelPosition": "right",
          "inline": false,
          "tableView": false,
          "values": [
            { "label": "Yes", "value": "true", "shortcut": "" },
            { "label": "No", "value": "false", "shortcut": "" }
          ],
          "validate": { "required": true },
