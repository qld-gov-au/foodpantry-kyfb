        {
          "label": "HTML",
          "attrs": [{ "attr": "", "value": "" }],
          "content": "<div class=\"alert alert-info\" role=\"alert\">\n  <h2><i class=\"fa fa-smile-o\"></i>Well done</h2>\n  <p>\n    Keeping a log of staff illness helps you prove you are not \n    responsible if a customer alleges food poisoning.\n  </p>\n</div>",
          "refreshOnChange": false,
          "tableView": false,
          "key": "html32",
          "conditional": { "show": true, "when": "staffLog", "eq": "true" },
